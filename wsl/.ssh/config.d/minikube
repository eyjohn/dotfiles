Host minikube
    ProxyCommand nc $(grep " server: " ~/.kube/config.minikube | sed 's/\s*server: https\?\:\/\/\([0-9a-z\:\.]\+\)\:[0-9]\+$/\1/i') 22
    User docker
    IdentityFile ~/winhome/.minikube/machines/minikube/id_rsa
    ControlMaster auto
    ControlPath ~/.ssh/sockets/%r@%h-%p
    ControlPersist yes
    UserKnownHostsFile=/dev/null
    StrictHostKeyChecking=no
    LogLevel QUIET
